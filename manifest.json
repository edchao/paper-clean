{
  "manifest_version": 2,

  "name": "Paper Clean Mode",
  "version": "0.1.0",
  "description": "Modify CSS and HTML",

  "content_scripts": [{
    "css": ["styles.css"],
    "js": ["content.js"],
    "matches": ["https://paper.dropbox.com/*"]
  }],

  "browser_action": {
    "default_icon": "icon.png"
  },

  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }

}
